@define-mixin color-config {
  /* Border API */
  --colorBorder: var(--ne-color-border, 0);

  /* Misc API */
  --colorBorderRadius: var(--ne-color-radius, 100%);
  --colorShadow: var(--ne-color-shadow, 0 1px 2px rgba(0, 0, 0, 0.16));
  --colorOutlineColor: var(--ne-color-outline-color, hsl(233, 64%, 85%));
  --colorOutlineWidth: var(--ne-color-outline-width, 0.2rem);
}
