    "elements",
    "components",
    "html",
    "css",
    "native",
    "npm",
    "ui",
    "kit"
  ],
  "size-limit": [
    {
      "webpack": false,
      "limit": "20 KB",
      "path": "dist/native-elements.css"
    }
  ],
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.7.7",
    "babel-eslint": "10.0.1",
    "babel-plugin-root-import": "6.2.0",
    "core-js": "3.6.0",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eslint": "5.16.0",
    "eslint-plugin-standard": "4.0.1",
    "npm-run-all": "4.1.5",
    "postcss-cli": "6.1.2",
    "postcss-easy-import": "3.0.0",
