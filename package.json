{
  "name": "@native-elements/native-elements",
  "version": "0.8.6",
  "description": "Native HTML Elements with CSS superpowers.",
  "main": "dist/native-elements.css",
  "repository": "https://github.com/equinusocio/native-elements",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/equinusocio/native-elements/issues"
  },
  "homepage": "https://github.com/equinusocio/native-elements",
  "author": {
    "name": "Equinusocio",
    "url": "http://equinsuocha.io"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "yarn rimraf dist/** dev/**",
    "predev": "yarn clean",
    "dev": "parcel index.html --open --out-dir dev --no-cache",
    "prebuild": "yarn clean",
    "build": "parcel build src/elements/native-elements/native-elements.pcss --no-minify --out-dir dist --detailed-report --no-source-maps",
    "postbuild": "copyfiles -f LICENSE README.md dist/ && size-limit",
    "prepublish": "yarn build",
    "size": "size-limit",
    "vars": "node scripts/extractVars.js"
  },
  "keywords": [
    "elements",
    "components",
    "html",
    "css",
    "native",
    "npm",
    "ui",
    "kit"
  ],
  "size-limit": [
    {
      "webpack": false,
      "limit": "20 KB",
      "path": "dist/native-elements.css"
    }
  ],
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.7.7",
    "babel-eslint": "10.0.1",
    "babel-plugin-root-import": "6.2.0",
    "chalk": "2.4.2",
    "copyfiles": "2.1.0",
    "core-js": "3.6.0",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eslint": "5.16.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "postcss-cli": "6.1.2",
    "postcss-easy-import": "3.0.0",
    "postcss-mixins": "6.2.1",
    "postcss-modules": "1.4.1",
    "postcss-preset-env": "6.6.0",
    "postcss-reporter": "6.0.1",
    "rimraf": "2.6.3",
    "size-limit": "1.3.2",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "what-input": "5.2.6"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ]
}
