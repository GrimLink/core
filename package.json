    "postcss-preset-env": "6.6.0",
    "postcss-reporter": "6.0.1",
    "rimraf": "2.6.3",
    "rollup": "1.27.14",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-serve": "1.0.1",
    "size-limit": "1.3.2",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "what-input": "5.2.6"
  },
  "resolutions": {
    "safe-eval": "^0.4.1",
    "serialize-to-js": "^3.0.1"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
