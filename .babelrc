  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage", // "usage" | "entry" | false, defaults to false.
        "corejs": "3.0.0",
        "targets": {
          "esmodules": true,
          "ie": "11"
        }
      }
    ]
  ],
  "plugins": [
    ["babel-plugin-root-import"]
  ]
}
